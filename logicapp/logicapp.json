{
    "definition": {
        "$schema": "https://schema.management.azure.com/providers/Microsoft.Logic/schemas/2016-06-01/workflowdefinition.json#",
        "actions": {
            "Initialize_variable": {
                "inputs": {
                    "variables": [
                        {
                            "name": "email_body",
                            "type": "string"
                        }
                    ]
                },
                "runAfter": {
                    "Parse_JSON": [
                        "Succeeded"
                    ]
                },
                "type": "InitializeVariable"
            },
            "Parse_JSON": {
                "inputs": {
                    "content": "@triggerBody()",
                    "schema": {
                        "attachment": "attachment"
                    }
                },
                "runAfter": {},
                "type": "ParseJson"
            },
            "Send_an_email_(V2)": {
                "inputs": {
                    "body": {
                        "Attachments": [
                            {
                                "ContentBytes": "@{base64(triggerBody()?['attachment'])}",
                                "Name": "vicgov_virtualmachines_audit.csv"
                            }
                        ],
                        "Body": "<p>@{variables('email_body')}</p>",
                        "Importance": "Normal",
                        "Subject": "Azure VM Audit ETL Pipeline Status",
                        "To": "lyoh001@gmail.com"
                    },
                    "host": {
                        "connection": {
                            "name": "@parameters('$connections')['office365']['connectionId']"
                        }
                    },
                    "method": "post",
                    "path": "/v2/Mail"
                },
                "runAfter": {
                    "Set_variable": [
                        "Succeeded"
                    ]
                },
                "type": "ApiConnection"
            },
            "Set_variable": {
                "inputs": {
                    "name": "email_body",
                    "value": "<!DOCTYPE html>\n<html>\n<style>\n    .notification-table-header {\n        width: 590px;\n        border-top: none;\n        background: #1170a4;\n        font-size: 11.0pt;\n        color: white;\n        font-weight: bold;\n        margin-left: 10px;\n        text-align: left;\n        border: none;\n        border-bottom: solid white 1.5pt;\n    }\n\n    .notification-table-text {\n        margin-left: 5px;\n        width: 70%;\n        text-align: left;\n        border: none;\n        border-bottom: solid white 1.5pt;\n        background: #F2F2F2;\n        font-size: 12.0pt;\n        height: 20.05pt;\n    }\n\n    .notification-card-footer span {\n        font-size: 12.0pt;\n        color: #7F7F7F;\n    }\n\n    .notification-card-footer p {\n        vertical-align: baseline;\n    }\n\n    .notification-body {\n        margin: 0 auto;\n        text-align: center;\n        width: 650px;\n        border: 1px black;\n        border-collapse: collapse;\n        background-color: #b6dbeb;\n    }\n</style>\n\n<body style='background-color: #dfdfdf;'>\n    <table align='center' style='width:100%;'>\n        <tr>\n            <td align='center' style='padding:0;'>\n                <div align='center'>\n                    <table align='left' class='notification-body'>\n                        <tr style='border: 1px grey; border-top:none; align-self: center;'>\n                            <td align='center'>\n                                <p style='font-size:5.0pt;'>\n                                    <span>&nbsp;</span>\n                                </p>\n                                <table align='center' style='width:595px;margin:0'>\n                                    <tr style='height:20.05pt'>\n                                        <td align='center' colspan='2' class='notification-table-header'\n                                            style='width:100%;'>\n                                            <img src='data:image/png;base64,'\n                                                style='vertical-align: bottom;' width='100%' height='100%' />\n                                        </td>\n                                    </tr>\n                                </table>\n                                <table align='center' style='width:595px;margin:0'>\n                                    <tr style='height:20.05pt'>\n                                        <td align='center' colspan='2' class='notification-table-header'\n                                            style='width:100%; height:20.05pt'>\n                                            <p style='text-align:center; font-size:16.0pt;'><b>VICGOV Azure VM Audit</b></p><br />&nbsp;</p>\n                                        </td>\n                                    </tr>\n                                    <tr class='notification-card-footer'>\n                                        <td align='center' colspan='2'>\n                                            </br></br>\n                                            <p style='text-align:left;'><b>VICGOV Azure VM Audit ETL Pipeline Status</b></p>\n                                            <p style='text-align:left;'>More details here: </p>\n                                        </td>\n                                    </tr>\n                                    <tr>\n                                        <td align='center' class='notification-table-header'>\n                                            <span>&nbsp; Status:</span>\n                                        </td>\n                                        <td align='center' class='notification-table-text'>\n                                            <span>&nbsp; Successful</span>\n                                        </td>\n                                    </tr>\n                                    <tr>                                    \n                                        <td align='center' class='notification-table-header'>\n                                            <span>&nbsp; Report Link:</span>\n                                        </td>\n                                        <td align='center' class='notification-table-text'>\n                                            <span>&nbsp; <a href='https://app.powerbi.com/links/3lzBHhln2G?ctid=722ea0be-3e1c-4b11-ad6f-9401d6856e24&pbi_source=linkShare' target='_blank'>Power BI Report View</a></span>\n                                        </td>\n                                    </tr>\n                                    <tr class='notification-card-footer'>\n                                        <td align='center' colspan='2'>\n                                            <p style='text-indent:36.0pt;'>\n                                                <span style='font-size:0.0pt;'>&nbsp;</span>\n                                            </p>\n                                            <p style='text-align:center;'>\n                                                <b>\n                                                    <span style='font-size:16.0pt;'>Cenitex</span>\n                                                </b>\n                                            </p>\n                                            <p style='text-align:center;'>\n                                                <span>Feedback:\n                                                    <a href='mailto:john.lee@cenitex.vic.gov.lyoh001@gmail.com <span\n                                                            style='font-size:11.0pt;colorlyoh001@gmail.com.vic.gov.au</span>\n                                                    </a>\n                                                </span>\n                                                <br />&nbsp;\n                                            </p>\n                                        </td>\n                                    </tr>\n                                </table>\n                            </td>\n                        </tr>\n                    </table>\n                </div>\n            </td>\n        </tr>\n    </table>\n</body>\n</html>"
                },
                "runAfter": {
                    "Initialize_variable": [
                        "Succeeded"
                    ]
                },
                "type": "SetVariable"
            }
        },
        "contentVersion": "1.0.0.0",
        "parameters": {
            "$connections": {
                "defaultValue": {},
                "type": "Object"
            }
        },
        "triggers": {
            "manual": {
                "inputs": {
                    "schema": {
                        "properties": {
                            "attachment": {
                                "type": "string"
                            }
                        },
                        "type": "object"
                    }
                },
                "kind": "Http",
                "type": "Request"
            }
        }
    },
    "parameters": {
        "$connections": {
            "value": {
                "office365": {
                    "connectionId": "/subscriptions/guid/resourceGroups/azurevmauditrg/providers/Microsoft.Web/connections/office365",
                    "connectionName": "office365",
                    "id": "/subscriptions/guid/providers/Microsoft.Web/locations/australiaeast/managedApis/office365"
                }
            }
        }
    }
}